.PHONY: clean All

All:
	@echo "----------Building project:[ ModifiedProgram - Debug ]----------"
	@cd "ModifiedProgram" && "$(MAKE)" -f  "ModifiedProgram.mk"
clean:
	@echo "----------Cleaning project:[ ModifiedProgram - Debug ]----------"
	@cd "ModifiedProgram" && "$(MAKE)" -f  "ModifiedProgram.mk" clean
