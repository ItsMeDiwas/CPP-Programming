.PHONY: clean All

All:
	@echo "----------Building project:[ SimplePointers - Debug ]----------"
	@cd "SimplePointers" && "$(MAKE)" -f  "SimplePointers.mk"
clean:
	@echo "----------Cleaning project:[ SimplePointers - Debug ]----------"
	@cd "SimplePointers" && "$(MAKE)" -f  "SimplePointers.mk" clean
